package com.srccodes.examples;

/**
 * @author yorkmiguel
 *
 */
public class TestMultiplication
{
    public static void main(String args[])
    {
        System.out.println("10 multiplied with 3 (Iterative) = "+MultiplicationByAdditionItr.multItr(10,3));
        System.out.println("10 multiplied with 3 (Recursive) = "+MultiplicationByAdditionRec.multRec(10,3));
    }
}
